<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.socialapp.domain.sub.AssemblyConstituancy" table="spm_assembly">
		<id column="asmc_id" name="id" type="java.lang.Long">
			<generator class="assigned" />
		</id>

		<property column="asmc_guid" name="guid" type="java.lang.String" />
		<property column="asmc_constituancyid" name="constituancyId" type="java.lang.Long" />
		<property column="asmc_constituancyname" name="constituancyName" type="java.lang.String" />
		<property column="asmc_created_date" name="createdOn" type="java.util.Date" />
		<property column="asmc_created_by" name="createdBy" type="java.lang.Long" />
		<property column="asmc_modified_date" name="modifiedOn" type="java.util.Date" />
		<property column="asmc_modified_by" name="modifiedBy" type="java.lang.Long" />
		<property column="asmc_active" name="active" type="java.lang.Long" />

	</class>
</hibernate-mapping>