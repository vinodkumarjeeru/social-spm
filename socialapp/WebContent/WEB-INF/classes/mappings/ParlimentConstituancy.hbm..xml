<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.socialapp.domain.sub.ParlimentConstituancy"
		table="spm_parliment">
		<id column="PMC_ID" name="id" type="java.lang.Long">
			<generator class="assigned" />
		</id>

		<property column="PMC_GUID" name="guid" type="java.lang.String" />
		<property column="PMC_CONSTITUANCYID" name="parlimentconstituancyId"
			type="java.lang.Long" />
		<property column="PMC_CONSTITUANCYNAME" name="parlimentconstituancyName"
			type="java.lang.String" />
		<property column="PMC_CREATED_DATE" name="createdOn" type="java.util.Date" />
		<property column="PMC_CREATED_BY" name="createdBy" type="java.lang.Long" />
		<property column="PMC_MODIFIED_DATE" name="modifiedOn" type="java.util.Date" />
		<property column="PMC_MODIFIED_BY" name="modifiedBy" type="java.lang.Long" />
		<property column="PMC_ACTIVE" name="active" type="java.lang.Long" />

	</class>
</hibernate-mapping><?xml version="1.0" encoding="UTF-8"?>